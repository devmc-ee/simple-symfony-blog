{% extends 'admin/adminbase.html.twig' %}
{% block body %}
    <h1>
        {{ title }}
    </h1>
    <a href="{{ path('admin_user_create') }}">Create user</a>
    <h2>Users list: </h2>
    <ul>
        {% for user in users %}
        <li> {{ user.email}}, {{ user.roles | json_encode }}</li>
        {% endfor %}
    </ul>
{% endblock %}