{% extends 'admin/index.html.twig' %}
{% block body %}
    <h1>
        {{ title }}
    </h1>

    <h2>Users list: </h2>
    <div class="col"><a href="{{ path('admin_user_create') }}">new user</a></div>
    <ul>
        {% for user in users %}
            <li> {{ user.email }}, {{ user.roles | json_encode }}</li>
        {% endfor %}
    </ul>
{% endblock %}